// French-Canadian keyboards, with additional German characters.
// Written by Jacques Michiels <jacquesmichiels@gmail.com>
partial alphanumeric_keys
xkb_symbols "fr-de" {

    include "ca(fr)"

    name[Group1] = "French (Canada, German characters)";

    key <AD03>	{ [   e,   E,   EuroSign ], [] };
    key <AC02>	{ [   s,   S,   ssharp   ], [] };

    include "level3(ralt_switch)"
};

